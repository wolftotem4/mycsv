{
    "name": "wolftotem4/mycsv",
    "description": "The PHP CLI tool helps you to convert the result from MySQL command to CSV format.",
    "license": "MIT",
    "keywords": [
        "csv",
        "mysql"
    ],
    "require": {
        "php": "^5.4 || ^7.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "MyCsv\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\MyCsv\\": "tests/"
        }
    }
}
